erv_DS2_Runtime_Put\":{\"value\":\"INFO\"},\"_App_TabServ_DS2_Runtime_SQL\":{\"value\":\"INFO\"},\"_App_TabServ_DS2_Runtime_Timing\":{\"value\":\"INFO\"},\"_App_tk_MAS\":{\"value\":\"INFO\"},\"_App_tk_MAS_CrossModCall\":{\"value\":\"INFO\"},\"_App_tk_MAS_CrossModCall_DS2\":{\"value\":\"INFO\"},\"_App_tk_MAS_Datagrid\":{\"value\":\"INFO\"},\"_App_tk_MAS_Datagrid_DS2\":{\"value\":\"INFO\"},\"_App_tk_MAS_GarbageCollection\":{\"value\":\"INFO\"},\"_App_tk_MAS_JNI\":{\"value\":\"INFO\"},\"_App_tk_MAS_ParallelExec\":{\"value\":\"INFO\"},\"_App_tk_MAS_Python\":{\"value\":\"INFO\"},\"_App_tk_MAS_Python_DS2\":{\"value\":\"INFO\"},\"_App_tk_MAS_RTrace\":{\"value\":\"INFO\"},\"_App_tk_MAS_Service\":{\"value\":\"INFO\"},\"_App_tk_MAS_SysRsrc\":{\"value\":\"INFO\"},\"_App_tk_MAS_Thread\":{\"value\":\"INFO\"},\"_Audit_Table_Connection\":{\"value\":\"INFO\"},\"_Driver_Name_Con\":\"_ALL_\",\"_Driver_Name_Stmt\":\"_ALL_\",\"_Python_Log_Level\":{\"value\":\"INFO\"},\"_Python_Logger\":\"\",\"_SCR_CONFIG\":{\"value\":\"INFO\"},\"_SCR_ENV\":{\"value\":\"INFO\"},\"_SCR_IO\":{\"value\":\"INFO\"},\"_SCR_MEM_CODE\":{\"value\":\"INFO\"},\"_SCR_MEM_NATIVE\":{\"value\":\"INFO\"},\"_SCR_MODULE_LOGGER\":{\"value\":\"INFO\"},\"_SCR_PROFILE\":{\"value\":\"INFO\"},\"_deployment_name\":\"\",\"_deployment_name_delete\":\"\",\"_deployment_name_restart\":\"\",\"_imagePull\":{\"value\":\"Always\"},\"_modus\":{\"value\":\"Pull image\"},\"_num_env_vars\":0,\"_pod_name\":\"\",\"_pub_destination\":{\"value\":\"AzureDocker-PG\"},\"_scr_endpoint\":\"\",\"_scr_tag\":\"latest\",\"_service_url\":\"k8s-scr-mgr.k8sscrmgr.svc.cluster.local\",\"_showrows\":10,\"_showrows_mas\":30,\"_showrows_rb\":{\"value\":\"All rows\"},\"_showrows_rb_mas\":{\"value\":\"Bottom rows\"}}}","templates":{"SAS":"/***************************************************************************\n* Custom Step: ID - K8S SCR Manager\n* --------------------------------------------------------------------------\n* The K8S SCR Manager node will:\n* - pull a SCR image into Kubernetes\n* - restart a pod running a SCR container\n* - delete a SCR deployment\n* - list all pods in the accosiated namespace\n* - get a SCR log \n* - get the MAS log \n* --------------------------------------------------------------------------\n* Note: Macros starting with an underscore contain UI variable values\n***************************************************************************/\n%let SERVICE_POINT=k8s-scr-mgr;\n\n/***************************************************************************/\n/* Macro pullscr()                                                         */ \n/* ----------------------------------------------------------------------- */\n/* The macro will generate generate the k8s YAML file and apply it to k8s  */\n/***************************************************************************/\n%macro pullscr();\n\tfilename py temp;\n\tdata _null_;\n\t    file py;\n\t\tput 'import requests';\n\t\tput ' ';\n        put 'import pandas as pd';\n        put 'def getLogEvnVariables():';\n        put '    vars= {\"Core Loggers\":{';\n        put '    \"_App_tk_MAS\":\"App.tk.MAS\",\"_App_tk_MAS_CrossModCall\":\"App.tk.MAS.CrossModCall\",\"_App_tk_MAS_CrossModCall_DS2\":\"App.tk.MAS.CrossModCall.DS2\",\"_App_tk_MAS_Datagrid\":\"App.tk.MAS.Datagrid\",';\n        put '    \"_App_tk_MAS_Datagrid_DS2\":\"App.tk.MAS.Datagrid.DS2\",\"_App_tk_MAS_GarbageCollection\":\"App.tk.MAS.GarbageCollection\",\"_App_tk_MAS_JNI\":\"App.tk.MAS.JNI\",\"_App_tk_MAS_ParallelExec\":\"App.tk.MAS.ParallelExec\",';\n        put '    \"_App_tk_MAS_Python\":\"App.tk.MAS.Python\",\"_App_tk_MAS_Python_DS2\":\"App.tk.MAS.Python.DS2\",\"_App_tk_MAS_RTrace\":\"App.tk.MAS.RTrace\",\"_App_tk_MAS_Service\":\"App.tk.MAS.Service\",';\n        put '    \"_App_tk_MAS_SysRsrc\":\"App.tk.MAS.SysRsrc\",\"_App_tk_MAS_Thread\":\"App.tk.MAS.Thread\",\"_Audit_Table_Connection\":\"Audit.Table.Connection\",';\n        put '    },\"REST Service Loggers\":{';\n        put '    \"_SCR_CONFIG\":\"SCR_CONFIG\",\"_SCR_ENV\":\"SCR_ENV\",\"_SCR_MEM_CODE\":\"SCR_MEM_CODE\",\"_SCR_MEM_NATIVE\":\"SCR_MEM_NATIVE\",\"_SCR_MODULE_LOGGER\":\"SCR_MODULE_LOGGER\",\"_SCR_PROFILE\":\"SCR_PROFILE\",\"_SCR_IO\":\"SCR_IO\",';\n        put '    },\"DS2 Run-Time Loggers\":{';\n        put '    \"_App_TabServ_DS2_Runtime_Calls\":\"App.TabServ.DS2.Runtime.Calls\",\"_App_TabServ_DS2_Runtime_SQL\":\"App.TabServ.DS2.Runtime.SQL\",\"_App_TabServ_DS2_Runtime_Timing\":\"App.TabServ.DS2.Runtime.Timing\",';\n        put '    \"_App_TabServ_DS2_Runtime_Put\":\"App.TabServ.DS2.Runtime.Put\",\"_App_TabServ_DS2_Runtime_Log\":\"App.TabServ.DS2.Runtime.Log\",';\n        put '    },\"DS2 SQL Services Loggers\":{';\n        put '    \"_App_SQLServ_API\":\"App.SQLServices.API\",\"_App_SQLServ_API_Environment\":\"App.SQLServices.API.Environment\",\"_App_SQLServ_API_Connection\":\"App.SQLServices.API.Connection\",\"_App_SQLServ_API_TF\":\"App.SQLServices.API.TF\",';\n        put '    \"_App_SQLServ_API_TF_TIMESTAMP\":\"App.SQLServices.API.TF.TIMESTAMP\",\"_App_SQLServ_API_DRV_NAME_Con\":\"App.SQLServices.API.DRIVER_NAME.Connection\",\"_App_SQLServ_API_DRV_NAME_Stmt\":\"App.SQLServices.API.DRIVER_NAME.Statement\",';\n        put '    },\"Python Logging\":{';\n        put '    \"_Python_Log_Level\":\"PY_LOG\"';\n        put '    }}';\n        put ' ';\n        put '    driver_name_con= SAS.symget(''_Driver_Name_Con'').strip()';\n        put '    if len(driver_name_con) == 0:';\n        put '        driver_name_con= ''_ALL_'' ';\n        put '    driver_name_stmt= SAS.symget(''_Driver_Name_Stmt'').strip()';\n        put '    if len(driver_name_stmt) == 0:';\n        put '        driver_name_stmt= ''_ALL_'' ';\n        put '    Python_Logger_name= SAS.symget(''_Python_Logger'').strip()';\n        put ' ';\n        put '    logVars= []';\n        put '    for category, logs in vars.items():';\n        put '        for var, logSwitch in logs.items():';\n        put '            varValue= SAS.symget(var).strip()';\n        put '            if varValue != ''INFO'':';\n        put '                if var == \"_App_SQLServ_API_DRV_NAME_Con\":';\n        put '                    logSwitch= logSwitch.replace(''DRIVER_NAME'', driver_name_con)';\n        put '                if var == \"_App_SQLServ_API_DRV_NAME_Stmt\":';\n        put '                    logSwitch= logSwitch.replace(''DRIVER_NAME'', driver_name_stmt)';\n        put '                logVars.append({category: {''SAS_SCR_LOG_LEVEL_''+logSwitch:varValue}})';\n        put ' ';\n        put '            if var == \"_Python_Log_Level\" and Python_Logger_name:';\n        put '                logSwitch= logSwitch.replace(''PY_LOG'', Python_Logger_name)';\n        put '                logVars.append({category: {''SAS_SCR_LOG_LEVEL_''+logSwitch:varValue}})';\n        put ' ';\n        put '    SAS.symput(''envlogtable'', ''N'')';\n        put '    if len(logVars) > 0:';\n        put '        # Put set environment variable into sas table in order to output later what vars are set ';\n        put '        env_var_category_lst= []';\n        put '        env_var_lst= []';\n        put '        env_var_value_lst= []';\n        put '        for logVariable in logVars:';\n        put '            for category, switch in logVariable.items():';\n        put '                for switch_var, switch_var_value in switch.items():';\n        put '                    env_var_category_lst.append(category)    ';\n        put '                    env_var_lst.append(switch_var)    ';\n        put '                    env_var_value_lst.append(switch_var_value)';\n        put ' ';\n        put '        data= {';\n        put '              ''Category'': env_var_category_lst,';\n        put '              ''Variable'': env_var_lst,';\n        put '              ''Value'': env_var_value_lst';\n        put '              }';\n        put '        df= pd.DataFrame(data)';\n        put '        SAS.df2sd(df, ''work.envlog'')';\n        put '        SAS.symput(''envlogtable'', ''Y'')';\n        put ' ';\n        put '    return logVars';        \n        put ' ';\n\t\tput 'SCR_NAME= SAS.symget(''_scr_name'').strip()';\n\t\tput 'SCR_TAG= SAS.symget(''_scr_tag'').strip()';\n\t\tput 'APP_OWNER= SAS.symget(''_app_owner'').strip()';\n\t\tput 'SERVICE_URL= SAS.symget(''_service_url'').strip()';\n\t\tput 'SERVICE_POINT= SAS.symget(''service_point'').strip()';\n\t\tput 'IMAGE_PULL_POLICY= SAS.symget(''_imagePull'').strip()';\n\t\tput 'PUB_DESTINATION= SAS.symget(''_pub_destination'').strip()';\n\t\tput 'SCR_ENDPOINT= SAS.symget(''_scr_endpoint'').strip()';\n\t\tput 'DEPLOYMENT_NAME= SAS.symget(''_deployment_name'').strip()';\n\t\tput 'ENV_VARS= []';\n        put ' ';\n        put 'env_var_lst= []';\n        put 'env_var_value_lst= []';\n        put '# get the 10 environment variables ';\n        put 'for i in range(1, 11):';\n\t\tput '    name= SAS.symget(f''_name_{i:02d}'').strip()';\n\t\tput '    value= SAS.symget(f''_value_{i:02d}'').strip()';\n\t\tput '    if len(name) > 0:';\n\t\tput '        env= {name:value}';\n\t\tput '        ENV_VARS.append(env)';\n        put '        # also store values in order to output later what env vars are set';\n        put '        env_var_lst.append(name)';\n        put '        env_var_value_lst.append(value)';\n        put '# write env vars to SAS table for output in the UI ';\n        put 'SAS.symput(''envtable'', ''N'')';\n        put 'if env_var_lst:';\n        put '    data= {';\n        put '          ''Variable'': env_var_lst,';\n        put '          ''Value'': env_var_value_lst';\n        put '          }';\n        put '    df= pd.DataFrame(data)';\n        put '    SAS.df2sd(df, ''work.env'')';\n        put '    SAS.symput(''envtable'', ''Y'')';\n        put ' ';\n        put '# get environment variables for SCR logging ';\n        put 'LOG_VARS=  getLogEvnVariables()';\n        put 'ENV_VARS= ENV_VARS +LOG_VARS';\n        put ' ';\n\t\tput